[
  {"key": "ctrl+alt+b","command": "workbench.action.tasks.runTask","args": "Run Back" },
  {"key": "ctrl+alt+f","command": "workbench.action.tasks.runTask","args": "Run Front" },
  {"key": "shift+f11","command": "workbench.action.toggleZenMode" },
  {"key": "ctrl+shift+q","command": "workbench.action.toggleMaximizedPanel" },
  {"key": "ctrl+t","command": "workbench.action.terminal.toggleTerminal" },
  {"key": "ctrl+x","command": "workbench.action.terminal.kill","when": "terminalFocus" },
  {"key": "ctrl+p","command": "workbench.action.terminal.focusPrevious","when": "terminalFocus" },
  {"key": "ctrl+n","command": "workbench.action.terminal.new","when": "terminalFocus" },
  {"key": "ctrl+f","command": "workbench.action.terminal.focusFindWidget","when": "terminalFocus" },
  {"key": "ctrl+`","command": "workbench.action.terminal.focus","when": "!terminalFocus" },
  {"key": "ctrl+`","command": "workbench.action.focusActiveEditorGroup","when": "terminalFocus" },
  {"key": "ctrl+k ctrl+u","command": "editor.action.transformToUppercase","when": "editorTextFocus && !editorReadonly" },
  {"key": "ctrl+k ctrl+l","command": "editor.action.transformToLowercase","when": "editorTextFocus && !editorReadonly" },
  {"key": "ctrl+alt+d","command": "editor.action.copyLinesDownAction","when": "editorFocus && !editorReadonly" },
  {"key": "ctrl+m","command": "editor.action.jumpToBracket","when": "editorFocus" },
  {"key": "ctrl+shift+/","command": "editor.action.blockComment","when": "editorFocus && !editorReadonly" }
]