[
  { "key": "f11", "command": "workbench.action.toggleZenMode" },
  { "key": "alt+b", "command": "gitlens.toggleFileBlame", "when": "editorFocus && !editorReadonly" },
  { "key": "ctrl+d", "command": "editor.action.copyLinesDownAction", "when": "editorFocus && !editorReadonly && !editorHasSelection" },
  { "key": "ctrl+f", "command": "workbench.action.terminal.focusFindWidget", "when": "terminalFocus" },
  { "key": "ctrl+m", "command": "editor.action.jumpToBracket", "when": "editorFocus" },
  { "key": "ctrl+p", "command": "workbench.action.terminal.focusPrevious", "when": "terminalFocus" },
  { "key": "ctrl+t", "command": "workbench.action.terminal.toggleTerminal" },
  { "key": "ctrl+w", "command": "-workbench.action.closeWindow", "when": "!editorIsOpen && !multipleEditorGroups"},
  { "key": "ctrl+x", "command": "workbench.action.terminal.kill", "when": "terminalFocus" },
  { "key": "ctrl+9", "command": "workbench.action.showAllEditors" },
  { "key": "ctrl+0", "command": "workbench.view.explorer" },
  { "key": "ctrl+`", "command": "-workbench.action.terminal.focus", "when": "!terminalFocus && editorFocus && !editorReadonly" },
  { "key": "ctrl+`", "command": "workbench.action.focusActiveEditorGroup", "when": "terminalFocus" },
  { "key": "alt+f7", "command": "references-view.findReferences", "when": "editorHasReferenceProvider"},
  { "key": "ctrl+k ctrl+m", "command": "workbench.action.editor.changeLanguageMode" },
  { "key": "ctrl+k ctrl+u", "command": "editor.action.transformToUppercase", "when": "editorTextFocus && !editorReadonly && editorHasSelection" },
  { "key": "ctrl+k ctrl+l", "command": "editor.action.transformToLowercase", "when": "editorTextFocus && !editorReadonly && editorHasSelection" },
  { "key": "ctrl+alt+c", "command": "copyRelativeFilePath", "when": "editorFocus" },
  { "key": "ctrl+shift+c", "command": "-workbench.action.terminal.openNativeConsole", "when": "!terminalFocus" },
  { "key": "ctrl+shift+d", "command": "editor.action.duplicateSelection", "when": "editorFocus && !editorReadonly && editorHasSelection" },
  { "key": "ctrl+shift+e", "command": "-workbench.view.explorer" },
  { "key": "ctrl+shift+f", "command": "-workbench.action.findInFiles" },
  { "key": "ctrl+shift+f", "command": "search.action.openEditor" },
  { "key": "ctrl+shift+g", "command": "workbench.view.scm" },
  { "key": "ctrl+shift+t", "command": "workbench.action.terminal.new", "when": "terminalFocus" },
  { "key": "ctrl+shift+w", "command": "-workbench.action.closeWindow" },
  { "key": "ctrl+shift+w", "command": "workbench.action.toggleMaximizedPanel", "when": "terminalFocus" },
  { "key": "escape escape", "command": "-workbench.action.exitZenMode", "when": "inZenMode" },
  { "key": "ctrl+shift+/", "command": "editor.action.blockComment", "when": "editorFocus && !editorReadonly" },
  { "key": "ctrl+shift+{", "command": "editor.fold", "when": "editorTextFocus" },
  { "key": "ctrl+shift+}", "command": "editor.unfold", "when": "editorTextFocus" },
  { "key": "ctrl+alt+l", "command": "editor.action.formatDocument", "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor" },
  { "key": "shift+alt+f", "command": "-editor.action.formatDocument", "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor" },
  { "key": "shift+alt+f12", "command": "-references-view.findReferences", "when": "editorHasReferenceProvider" }
]
